<h1>New pay_period</h1>

<% form_for(@pay_period) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :begins %><br />
    <%= f.datetime_select :begins %>
  </p>
  <p>
    <%= f.label :ends %><br />
    <%= f.datetime_select :ends %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', pay_periods_path %>