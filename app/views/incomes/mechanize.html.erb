  require 'rubygems'
  require 'mechanize'

  a = Mechanize.new { |agent|
    agent.user_agent_alias = 'Mac Safari'
  }

  a.get('http://www.usbank.com/en/PersonalHome.cfm/') do |page|
    search_result = page.form_with(:name => 'logon2') do |search|
